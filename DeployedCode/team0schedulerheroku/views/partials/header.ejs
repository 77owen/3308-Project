<link rel="stylesheet" href="/css/Navigation-with-Button.css">
<link rel="stylesheet" href="/css/styles.css">
<script src="https://kit.fontawesome.com/f1905881c2.js" crossorigin="anonymous"></script>
<script src="/bootstrap/js/bootstrap.min.js"></script>
<script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
<script src="/js/Scheduler.js"></script>

  </head>
  <% if(dark){%>
    <body class="bg-secondary text-white" style="background: #c0c2c4;">
        <nav class="navbar navbar-dark bg-darkn navbar-expand-lg bg-dark">
  <%}else{%>
    <body style="background: #c0c2c4;">
        <nav class="navbar navbar-light bg-lightn navbar-expand-lg bg-light">
  <%}%>
        <div class="container-fluid"><a class="navbar-brand logo" href="/home">CU Class Scheduler</a><button data-toggle="collapse" class="navbar-toggler" data-target="#navcol-1"><span class="visually-hidden">Toggle navigation</span><span class="navbar-toggler-icon"></span></button>
            <div class="collapse navbar-collapse" id="navcol-1">
                <ul class="navbar-nav ms-auto">
                    <!-- <li class="nav-item"><a class="nav-link" href="/cart">Cart</a></li> -->
                    <li class="nav-item"><a class="nav-link" href="/schedule">Schedule</a></li>
                    <li class="nav-item"><a class="nav-link" href="/login">Login</a></li>
                </ul>
                <div class="custom-control custom-switch mr=5">
                    <input type="checkbox" class="custom-control-input" id="selector" name="selector" value="selector" <% if(dark){%> checked <%}%>>
                    <label class="custom-control-label" for="selector">Dark Mode</label>
                </div>
            </div>
        </div>
    </nav>
